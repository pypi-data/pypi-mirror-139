    <td>{{ vrf }}</td>
    <td>{{ address_family }}</td>
    <td>{{ to_parse_vrf[vrf].route_distinguisher }}</td>
  </tr>
{%-                 endfor %}
{%-             endfor %}
  </tbody>  
</table>
</body></html>
{%-         else %}
# Learn VRF
{%-             for vrf in to_parse_vrf %}
## {{ vrf }}
