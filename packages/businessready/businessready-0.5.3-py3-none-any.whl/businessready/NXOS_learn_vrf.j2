{%-             endfor %}
{%-         endfor %}
{%-     else %}
{%-         if filetype_loop == 2 %}
<html><body>
<h1>Learn VRF</h1>
<table>
  <thead>
  <tr>
    <th>VRF</th>
    <th>Address Family</th>
    <th>Route Distinguisher</th>
  </tr>
  </thead>
  <tbody>
{%-             for vrf in to_parse_vrf %}
