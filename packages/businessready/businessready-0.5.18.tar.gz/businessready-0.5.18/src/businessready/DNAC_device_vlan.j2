{%-     else %}
{%-         if filetype_loop == 2 %}
<html><body>
<h1>Device VLANs</h1>
<table>
  <thead>
    <tr>
      <th>VLAN Number</th>
      <th>Interface Name</th>
      <th>IP Address</th>
      <th>Network Address</th>
      <th>Number of IPs</th>
    </tr>
  </thead>
  <tbody>
{%-     for item in vlan %}
  <tr>
{%-         if item.ipAddress %}
    <td>{{ item.vlanNumber }}</td>
    <td>{{ item.interfaceName }}</td>
    <td>{{ item.ipAddress }} / {{ item.prefix }}</td>
    <td>{{ item.networkAddress }}</td>
    <td>{{ item.numberOfIPs }}</td>
{%-         else %}
    <td>{{ item.vlanNumber }}</td>
    <td>{{ item.interfaceName }}</td>
    <td>N/A</td>
    <td>N/A</td>
    <td>N/A</td>
{%-         endif %}
  </tr>
{%-     endfor %}
  </tbody>
</table>
</body></html>
{%-         else %}
# VLANs
{%-     for item in vlan %}
## {{ item.vlanNumber }}
### Name: {{ item.interfaceName }}
{%-         if item.ipAddress %}
