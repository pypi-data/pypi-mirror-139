{%- if filetype_loop == 0 %}VRF,Protocols,Interfaces
{%-     for vrf in to_parse_vrf %}
{%-         for interface in to_parse_vrf[vrf].interfaces %}
{%-             for protocol in to_parse_vrf[vrf].protocols %}
{%-                 if interface != "ipv4" %}
{{ vrf }},{{ protocol }},{{ interface }}
{%-                 endif %}
{%-             endfor %}
{%-         endfor %}
{%-     endfor %}
{%- else %}
{%-     if filetype_loop == 1 %}
# Show VRF
| VRF | Protocol | Interfaces |
| --- | -------- | ---------- |
{%-         for vrf in to_parse_vrf %}
{%-             for interface in to_parse_vrf[vrf].interfaces %}
{%-                 for protocol in to_parse_vrf[vrf].protocols %}
{%-                     if interface != "ipv4" %}
| {{ vrf }} | {{ protocol }} | {{ interface }} |
{%-                     endif %}
{%-                 endfor %}
{%-            endfor %}
{%-        endfor %}
{%-     else %}
{%-         if filetype_loop == 2 %}
<html><body>
<h1>Show VRF</h1>
<table>
  <thead>
  <tr>
    <th>VRF</th>
    <th>Protocols</th>
    <th>Interfaces</th>
  </tr>
  </thead>
  <tbody>
{%-             for vrf in to_parse_vrf %}
{%-                 for interface in to_parse_vrf[vrf].interfaces %}
{%-                     for protocol in to_parse_vrf[vrf].protocols %}
{%-                         if interface != "ipv4" %}
  <tr>
    <td>{{ vrf }}</td>
    <td>{{ protocol }}</td>
    <td>{{ interface }}</td>
  </tr>
{%-                         endif %}
{%-                     endfor %}
{%-                 endfor %}
{%-             endfor %}
  </tbody>
</table>
</body></html>
{%-         else %}
# Show VRF
{%-             for vrf in to_parse_vrf %}
## {{ vrf }}
{%-                 for interface in to_parse_vrf[vrf].interfaces %}
### {{ interface }}
{%-                     for protocol in to_parse_vrf[vrf].protocols %}
#### {{ protocol }}
{%-                     endfor %}
{%-                 endfor %}
{%-             endfor %}
{%-         endif %}
{%-     endif %}
{%- endif %}